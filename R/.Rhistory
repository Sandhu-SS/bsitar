in_subdirectory <- function(string, wd = getwd()){
if(grepl(string, wd)){
packpat <- paste0(string, "$")
while(!grepl(packpat, wd)){
wd <- dirname(wd)
if(!grepl(string, wd))
return(NULL)
}
if(basename(wd) == string)
wd
else
## the found directory has `string' as a suffix, eg. xxxRdpack, not Rdpack
NULL
}else
NULL
}
